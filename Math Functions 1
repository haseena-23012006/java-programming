import java.util.Scanner;

public class Sample {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);

        float floatValue = scanner.nextFloat();
        int sqrtValue = scanner.nextInt();
        int base = scanner.nextInt();
        int power = scanner.nextInt();

        System.out.println((int) Math.floor(floatValue));
        System.out.println((int) Math.ceil(floatValue));
        System.out.println((int) Math.sqrt(sqrtValue));
        System.out.println((int) Math.pow(base, power));
    }
}
