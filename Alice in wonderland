import java.util.Scanner;

public class path {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int num=sc.nextInt();
        int num1=num/10;  
        int num2=num%10; 
        int sum=num1+num2;

        System.out.println("Bird said:" + num);
        System.out.println("Alice must go in path-" + sum);
    }
}
