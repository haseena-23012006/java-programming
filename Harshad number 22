import java.util.Scanner;
public class Harshad {
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        int num=n;
        int sum=0;
        while(n!=0){
            sum=n % 10;
            n=n/10;
        }
        if(num%sum == 0){
            System.out.println("Harshad Number");
        }else{
            System.out.println("Not Harshad Number");
        }
    }
}
